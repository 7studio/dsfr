<div class="<%= prefix %>-footer__brand <%= prefix %>-enlarge-link" >
	<% if (!locals.logo) { %>
		<a href="/" title="Retour à l’accueil">
	<% } %>
			<%- include( '../../../../logo/src/templates/ejs/logo', locals.brand.logo); %>
	<% if (!locals.logo) { %>
		</a>
	<% } else { %>
		<a class="<%= prefix %>-footer__brand-link" href="/" title="Retour à l’accueil">
			<!-- L'alternative de l'image (attribut alt) doit impérativement être renseignée et reprendre le texte visible dans l’image -->
			<img class="<%= prefix %>-footer__logo" src="<%= logo.src %>" alt="<%= logo.alt %>">
		</a>
	<% } %>
</div>

